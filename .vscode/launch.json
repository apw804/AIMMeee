  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "AIMMee: template.yaml",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/AIMMee/AIMMee.py",
      "args": [
        "-c",
        "config/_templates/AIMM_simulator.yaml"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }

    
  ]
}